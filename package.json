{
	"name": "@adcalls/n8n-nodes-call-tracking",
	"version": "0.1.0",
	"description": "These nodes let you integrate your AdCalls call-tracking with n8n.",
	"license": "MIT",
	"homepage": "",
	"keywords": [
		"n8n-community-node-package"
	],
	"author": {
		"name": "AdCalls",
		"email": "dev@adcalls.nl"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/AdCalls/n8n-nodes-call-tracking.git"
	},
	"scripts": {
		"build": "n8n-node build",
		"build:watch": "tsc --watch",
		"dev": "n8n-node dev",
		"lint": "n8n-node lint",
		"lint:fix": "n8n-node lint --fix",
		"release": "n8n-node release",
		"prepublishOnly": "n8n-node prerelease"
	},
	"files": [
		"dist"
	],
	"n8n": {
		"n8nNodesApiVersion": 1,
		"strict": true,
		"credentials": [
			"dist/credentials/AdCallsWebhookApi.credentials.js"
		],
		"nodes": [
			"dist/nodes/AdCallsHook/AdCallsHook.node.js"
		]
	},
	"devDependencies": {
		"@n8n/node-cli": "*",
		"@types/node": "^25.2.0",
		"eslint": "9.32.0",
		"prettier": "3.6.2",
		"release-it": "^19.0.4",
		"typescript": "5.9.2"
	},
	"peerDependencies": {
		"n8n-workflow": "*"
	}
}
